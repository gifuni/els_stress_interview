dti_6mo %>% 
  filter(!is.na(touch_M)) %>%
  gather(fornix, value,  ifo_l_AD:ifo_l_RD) %>% 
  ggplot(aes(sens_touch, value)) +
  geom_jitter() +
  geom_smooth(method = "lm", se = FALSE) +
  facet_wrap(.~fornix, scales = "free") +
  labs(title = "6-month Inferior Fronto-Occipital Fasciculus ~ Maternal sensitive touch")


dti_6mo %>% 
  filter(!is.na(touch_M)) %>%
  gather(tr, value,  thal_t_l_AD:thal_t_r_RD) %>% 
  ggplot(aes(sens_touch, value)) +
  geom_jitter() +
  geom_smooth(method = "lm", se = FALSE) +
  facet_wrap(.~tr, scales = "free") +
  labs(title = "6-month Thalamic Radiation Temporal ~ Maternal sensitive touch")
```

