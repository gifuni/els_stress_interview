```{r}
dti_newborn %>% 
  gather(uncinate, value, unc_l_AD:unc_r_RD) %>% 
  ggplot(aes(hcc_overall_mean, value)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  facet_wrap(.~uncinate, scales = "free") +
  labs(title = "Newborn Uncinate Fasciculus ~ HCC")

dti_newborn %>% 
  gather(cc, value, cc_full_AD:cc_full_RD) %>% 
  ggplot(aes(hcc_overall_mean, value)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  facet_wrap(.~cc, scales = "free") +
  labs(title = "Newborn Corpus Callosum Full ~ HCC")

dti_newborn %>% 
  gather(cgc, value, cgc_l_AD:cgc_r_RD) %>% 
