  geom_smooth(method = "lm", se = FALSE) +
  labs(
    y = "L. amygdala - L. OFC ",
    x = "Maternal sensitivity"
  )

l_amyg %>% 
  ggplot(aes(sens_M, clust68)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  labs(
    y = "L. amygdala - R. DLPFC ",
    x = "Maternal sensitivity"
  )

l_amyg %>% 
  ggplot(aes(sens_M, clust71)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  labs(
    y = "L. amygdala - L. DLPFC ",
    x = "Maternal sensitivity"
  )

r_amyg %>% 
  ggplot(aes(sens_M, clust40)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  labs(
    y = "R. amygdala - L. DLPFC ",
    x = "Maternal sensitivity"
  )

r_amyg %>% 
  filter(!is.na(sens_M)) %>% 
  ggplot(aes(sens_M, clust40)) +
  geom_point(size = 6) +
  geom_smooth(method = "lm", se = FALSE, size = 4) +
  scale_x_continuous(breaks = seq.int(1, 6, .5)) +
  #scale_y_continuous(breaks = seq.int(.19, .30, .01)) +
  theme_minimal() +
  theme(
    axis.title = element_text(size = 25),
    axis.text = element_text(size = 22) 
  ) +
  labs(
    x = "Maternal sensitivity",
    y = "R. amygdala - L. DLPFC "
  )

ggsave("~/Desktop/BABIES/r01_winter_2019/plots/6moDLPFCrs_sens.png", width = 7, height = 6)
```


